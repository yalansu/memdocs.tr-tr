{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
         
	  "**/includes/**",
          "sccm/**"   
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**",
          "**/*.png",
	  "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "sccm/**"
        ]
      }
          ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/mem/breadcrumb/toc.json",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/memdocs.tr-tr",
       "brand": "Microsoft Endpoint Manager",
      "searchScope": ["Intune"]
      ,
      "disable_contribution_on_translation_quality": [
        "MT",
        "MTE75",
        "MTE95",
        "Blank"
      ]
    },
    "fileMetadata": {
     "feedback_product_url": {
        "configmgr/**/*.md": "https://configurationmanager.uservoice.com/",
        "intune/**/*.md": "https://microsoftintune.uservoice.com/"
      },
      "searchScope": {
        "configmgr/**/*.md": ["ConfigMgr"],
        "configmgr/core/**/*.md": ["ConfigMgr"]        
      }, 
      
      "featureFlags": {
        "*.md" : [
          "show_binary_rating"
        ]
    
          },
      "feedback_system": {
        "configmgr/core/misc/**/*": "None"
      }
    },
    "template": [],
    "dest": "memdocs",
    "markdownEngineName": "markdig"
  }
}
